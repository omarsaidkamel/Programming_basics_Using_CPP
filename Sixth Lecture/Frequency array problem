#include <iostream>
using namespace std;
int n,m,h,a[1000006];
int main(){
    cin>>n>>m; // O(N^2)
    for(int i=0;i<n;i++) cin>>a[i];
    for(int i=1;i<=m;i++){int c=0;
       for(int j=0;j<n;j++){
          if(a[j]==i)c++;
       }
      cout<<c<<"\n";
    }
    return 0;
}

