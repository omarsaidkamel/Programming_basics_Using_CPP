#include <iostream>
using namespace std;
int main(){
    long long int n;
    cin>>n;
    if(n==0||n==1)cout<<1;
    else if(n==2)cout<<2;
    else if(n==3)cout<<6;
    else if(n==4)cout<<4;
    else cout<<0;
    //0 = 1
    //1 = 1
    //2 = 2
    //3 = 6
    //4 = 24 => 4
    //5 = 120 => 0
    //6 = 720 => 0
    //7 
    
    return 0;
}
